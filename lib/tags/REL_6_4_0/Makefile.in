srcdir 	= @srcdir@/src
VPATH	= @srcdir@
incdir	= @srcdir@/include

prefix	= @prefix@
exec_prefix	= @exec_prefix@
TARGETLIB	= @libdir@
TARGETBIN	= @bindir@
TARGETINCLUDE	= @includedir@
