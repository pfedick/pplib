###########################################################################
###
### PPL7 Makefile
###
###########################################################################
### Global Options
###########################################################################
srcdir 	= @srcdir@/src
VPATH	= @srcdir@
incdir	= @srcdir@/include

prefix	= @prefix@
exec_prefix	= @exec_prefix@
TARGETLIB	= @libdir@
TARGETBIN	= @bindir@
TARGETINCLUDE	= @includedir@
CC			= @CC@
CXX			= @CXX@
CFLAGS		=  -I${incdir} -DPPL7LIB @CFLAGS@ @DEFS@  @PTHREAD_CFLAGS@ 	@ZLIB_CFLAGS@ @BZ2_CFLAGS@ @PCRE_CFLAGS@ @openssl_includes@ @ICONV_CFLAGS@ @MYSQL_CFLAGS@ @PGSQL_CFLAGS@ 	@LIBCURL_CPPFLAGS@  @LAME_CFLAGS@ @MPG123_CFLAGS@ 	@GRAPHIC_CFLAGS@ @SOUND_CFLAGS@ @LIBMCRYPT_CFLAGS@ @LIBMHASH_CFLAGS@ @LIBLDNS_CFLAGS@
	
	
###	@X_CFLAGS@   @SYBASE_CFLAGS@   @MAD_CFLAGS@ 
CPPFLAGS	= @CPPFLAGS@
LDFLAGS		= @LDFLAGS@
LIBS		= @openssl_libs@ @LIBS@ @PTHREAD_CFLAGS@ @PTHREAD_LIBS@ @ZLIB_CFLAGS@ @BZ2_LIBS@ @PCRE_LIBS@ 	@ICONV_LIBS@ @MYSQL_LIBS@ @PGSQL_LIBS@ @LIBCURL@ @LAME_LIBS@ @GRAPHIC_LIBS@ @SOUND_LIBS@ @LIBMCRYPT_LIBS@ @LIBMHASH_LIBS@ @LIBLDNS_LIBS@ @MPG123_LIBS@
	
###  @SYBASE_LIBS@  @MAD_LIBS@
LIBNAME		= libppl7
LIBDEBUG	= libppl7-debug
DISTNAME	= ppl
ASM			= @ASM@ @ASMFLAGS@
ASMDEBUGFLAGS	= @ASMDEBUGFLAGS@	
###########################################################################
### Object Files
###########################################################################

RELEASE	= release/AVLTree.o \
	release/Compat.o \
	release/Dir.o \
	release/DirEntry.o \
	release/Exceptions.o \
	release/File.o \
	release/FileObject.o \
	release/Functions.o \
	release/Heap.o \
	release/MemFile.o \
	release/Mutex.o \
	release/StringFunctions.o \
	release/Threads.o \
	release/type_Array.o \
	release/type_AssocArray.o \
	release/type_ByteArray.o \
	release/type_ByteArrayPtr.o \
	release/type_DateTime.o \
	release/type_Pointer.o \
	release/type_String.o \
	release/type_Variant.o \
	release/crc32.o \
	release/md5.o \
	release/random.o \
	 \
	 \
	 \
	 \
	release/sound_Icecast.o

DEBUG	= debug/AVLTree.o \
	debug/Compat.o \
	debug/Dir.o \
	debug/DirEntry.o \
	debug/Exceptions.o \
	debug/File.o \
	debug/FileObject.o \
	debug/Functions.o \
	debug/Heap.o \
	debug/MemFile.o \
	debug/Mutex.o \
	debug/StringFunctions.o \
	debug/Threads.o \
	debug/type_Array.o \
	debug/type_AssocArray.o \
	debug/type_ByteArray.o \
	debug/type_ByteArrayPtr.o \
	debug/type_DateTime.o \
	debug/type_Pointer.o \
	debug/type_String.o \
	debug/type_Variant.o \
	debug/crc32.o \
	debug/md5.o \
	debug/random.o \
	 \
	 \
	 \
	 \
	debug/sound_Icecast.o

ASM_OBJ_RELEASE	=	
ASM_OBJ_DEBUG	=	


@HAVE_X86_ASSEMBLER@

###########################################################################
### Targets
###########################################################################


release: release/$(LIBNAME).a

debug: debug/$(LIBDEBUG).a

all:	install_debug install

debug/$(LIBDEBUG).a: debug/make $(DEBUG) $(ASM_DEBUG)
	ar -r debug/$(LIBDEBUG).a $(DEBUG) $(ASM_DEBUG)
	-chmod a+x ppl7-config

release/$(LIBNAME).a: release/make $(RELEASE) $(ASM_RELEASE)
	ar -r release/$(LIBNAME).a $(RELEASE) $(ASM_RELEASE)
	-chmod a+x ppl7-config

debug/make:
	-mkdir -p debug
	-touch debug/make

release/make:
	-mkdir -p release
	-touch release/make

install:	release/$(LIBNAME).a
	-cp release/$(LIBNAME)*.a $(TARGETLIB)
	-cp $(incdir)/ppl7.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-types.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-algorithms.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-exceptions.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-inet.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-grafix.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-tk.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-sound.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-crypt.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-db.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-unixconfig.h $(TARGETINCLUDE)/ppl7-config.h
	-chmod a+x ppl7-config  
	-cp ppl7-config $(TARGETBIN)

install_debug: debug/$(LIBDEBUG).a
	-cp debug/$(LIBDEBUG)*.a $(TARGETLIB)
	-cp $(incdir)/ppl7.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-types.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-algorithms.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-exceptions.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-inet.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-grafix.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-tk.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-sound.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-crypt.h $(TARGETINCLUDE)
#	-cp $(incdir)/ppl7-db.h $(TARGETINCLUDE)
	-cp $(incdir)/ppl7-unixconfig.h $(TARGETINCLUDE)/ppl7-config.h
	-chmod a+x ppl7-config
	-cp ppl7-config $(TARGETBIN)

clean:
	-rm -rf release
	-rm -rf debug
	-rm -rf documentation
	-rm -rf tests/bin

deinstall:
	-rm -f $(TARGETINCLUDE)/ppl7*.h
	-rm -f $(TARGETLIB)/$(LIBDEBUG)*.a
	-rm -f $(TARGETLIB)/$(LIBNAME)*.a
	-rm -f $(TARGETBIN)/ppl7-config
	
distclean: clean
	-rm -rf $(incdir)/ppl7-unixconfig.h $(incdir)/config.h config.log config.status autom4te.cache configure.scan

configure: genConfigure genMakefile.in configure.in
	-sh genConfigure

Makefile:
	-sh genMakefile.in
	
doku: html

html:
	-mkdir -p documentation
	-rm -rf documentation/html
	-mkdir -p documentation/html
	-doxygen Doxyfile
	-cp docs/header-bg.png documentation/html

test:
	- cd tests; make all
	 

